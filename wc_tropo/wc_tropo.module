<?php
function wc_tropo_wc_config($config) {	
  $config['service'] = array(
  	'tropo' => array(
  		'name'							=> 'tropo',
  		'accessTokenURL'		=> '',
  		'authenticateURL'		=> '',
  		'authorizeURL'			=> '',
  		'requestTokenURL'		=> '',
			
			// Set it to use the API Key instead of OAuth
			'authMethod'				=> 'apiKey',
		),
  );
  return $config;
}